{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synthMain": "cdk synth HogwashStack",
    "deployMain": "cdk deploy HogwashStack --require-approval never",
    "destroyMain": "aws s3 rm s3://hogwash-frontend-bucket --recursive 1>/dev/null 2>&1; cdk destroy HogwashStack -f",
    "synthHostedZone": "cdk synth HogwashHostedZone",
    "deployHostedZone": "cdk deploy HogwashHostedZone --require-approval never",
    "destroyHostedZone": "cdk destroy HogwashHostedZone -f"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.118.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^10.17.60",
    "aws-cdk": "^1.118.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.118.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.118.0",
    "@aws-cdk/aws-dynamodb": "^1.118.0",
    "@aws-cdk/aws-iam": "^1.118.0",
    "@aws-cdk/aws-lambda": "^1.118.0",
    "@aws-cdk/aws-logs": "^1.118.0",
    "@aws-cdk/aws-route53": "^1.118.0",
    "@aws-cdk/aws-s3": "^1.118.0",
    "@aws-cdk/aws-s3-deployment": "^1.118.0",
    "@aws-cdk/core": "^1.118.0",
    "source-map-support": "^0.5.16"
  }
}
