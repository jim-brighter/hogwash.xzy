{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deployBackend": "cdk deploy HogwashBackend --require-approval never --outputs-file backend-outputs.json",
    "destroyBackend": "cdk destroy HogwashBackend -f",
    "deployFrontend": "cdk deploy HogwashFrontend --require-approval never --outputs-file frontend-outputs.json",
    "destroyFrontend": "cdk destroy HogwashFrontend -f",
    "deployHostedZone": "cdk deploy HogwashHostedZone --require-approval never --outputs-file hosted-zone-outputs.json",
    "destroyHostedZone": "cdk destroy HogwashHostedZone -f"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.118.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^10.17.60",
    "aws-cdk": "^1.118.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2": "^1.118.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.118.0",
    "@aws-cdk/aws-certificatemanager": "^1.118.0",
    "@aws-cdk/aws-dynamodb": "^1.118.0",
    "@aws-cdk/aws-iam": "^1.118.0",
    "@aws-cdk/aws-lambda": "^1.118.0",
    "@aws-cdk/aws-logs": "^1.118.0",
    "@aws-cdk/aws-route53": "^1.118.0",
    "@aws-cdk/aws-route53-targets": "^1.118.0",
    "@aws-cdk/aws-s3": "^1.118.0",
    "@aws-cdk/aws-s3-deployment": "^1.118.0",
    "@aws-cdk/core": "^1.118.0",
    "source-map-support": "^0.5.16"
  }
}
